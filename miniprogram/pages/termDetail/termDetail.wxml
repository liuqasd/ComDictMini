<view class="container">
  <view class="term-header">
    <text class="term-title">{{term.word}}</text>
  </view>
  <view class="term-content">
    <view class="term-info">
      <text>翻译：{{term.translation}}</text>
    </view>
    <view class="term-example">
      <text>例句：{{term.example}}</text>
    </view>
  </view>
</view>
